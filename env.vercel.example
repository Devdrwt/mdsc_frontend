# Variables d'environnement pour Vercel
# Copiez ce fichier vers .env.local et remplissez les valeurs

# API Backend URL (sera l'URL de votre API déployée sur Vercel)
NEXT_PUBLIC_API_URL=https://votre-api-vercel.vercel.app/api

# Google OAuth (même que pour le développement)
NEXT_PUBLIC_GOOGLE_CLIENT_ID=votre_google_client_id

# Configuration pour la production
NODE_ENV=production
